{
  "name": "@vscode/test-electron",
  "version": "2.4.0",
  "bin": {
    "vscode-test": "./out/cli-runner/bin.js"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "prepack": "tsc -p ./",
    "fmt": "prettier --write \"lib/**/*.ts\" \"*.md\"",
    "test": "eslint lib --ext ts && vitest && tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "main": "./out/index.js",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@types/mocha": "^10.0.1",
    "chokidar": "^3.5.3",
    "glob": "^10.3.5",
    "http-proxy-agent": "^7.0.0",
    "https-proxy-agent": "^7.0.2",
    "jszip": "^3.10.1",
    "minimatch": "^9.0.3",
    "mocha": "^10.2.0",
    "semver": "^7.5.2",
    "supports-color": "^8.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@types/semver": "^7.3.13",
    "@types/supports-color": "^8.1.1",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-plugin-header": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "license": "MIT",
  "author": "Visual Studio Code Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-test.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-test/issues"
  }
}
